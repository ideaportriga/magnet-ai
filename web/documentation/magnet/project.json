{
  "name": "magnet-docs",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "documentation/magnet",
  "// targets": "to see all targets run: nx show project magnet-admin --web",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn vitepress dev documentation/magnet"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn vitepress build documentation/magnet"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn vitepress preview documentation/magnet"
      }
    }
  }
}
